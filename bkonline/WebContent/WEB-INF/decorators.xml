<decorators defaultdir="/WEB-INF/decorators">
	<excludes>
		<pattern>*ajax*</pattern>
		<pattern>*Ajax*</pattern>
		<pattern>/jsp/template/viewer.jsp</pattern>
	</excludes>

	<decorator name="login" page="login.jsp">
		<pattern>/jsp/security/permission.jsp</pattern>
		<pattern>/jsp/security/lineprofile.jsp</pattern>
		<pattern>/jsp/security/barcode.jsp</pattern>
	</decorator>
	<decorator name="main" page="main.jsp">
		<pattern>/jsp/*.jsp</pattern>
	</decorator>
	<decorator name="calendar" page="calendar.jsp">
		<pattern>/jsp/calendar/dashboard/dashboard.jsp</pattern>
	</decorator>
	<decorator name="component" page="component.jsp">
		<pattern>/jsp/component/*.jsp</pattern>
	</decorator>
</decorators>