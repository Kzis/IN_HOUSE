        <systemCode>BK</systemCode>
        <defaultSecUserIds>21</defaultSecUserIds>
        <datetimeLocaleString>en</datetimeLocaleString>
        <applicationLocaleString>th</applicationLocaleString>
    </application>

    <attachFile>
        <systemPath>BK/</systemPath>
    </attachFile>
    <report>
    	<systemPath>BK/</systemPath>
    </report>
    <contextConfig>
	    <url>http://127.0.0.1:8080/</url><!-- สำหรับเรียกโหลด js และ css -->
